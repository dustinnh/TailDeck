    "lint-staged": "^16.2.7",
    "pino-pretty": "^13.1.2",
    "postcss": "^8",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "prisma": "^6.19.0",
